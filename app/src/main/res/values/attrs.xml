<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InputEditText">
        <attr name="android:inputType" />
        <attr name="android:textAppearance" />
        <attr name="android:drawableRight" />
        <attr name="android:focusable" />
        <attr name="ietErrorEnabled" format="boolean" />
        <attr name="ietHintEnabled" format="boolean" />
        <attr name="ietText" format="string" />
        <attr name="ietHint" format="string" />
        <attr name="ietError" format="string" />
        <attr name="ietLines" format="integer" />
        <attr name="ietMaxLines" format="integer" />
        <attr name="android:imeOptions" />
        <attr name="showBottomLine" format="boolean" />
        <attr name="ietPasswordToggleEnabled" format="boolean" />
        <attr name="ietMinLength" format="integer" />
        <attr name="ietMaxLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="LabelInputEditText">
        <attr name="android:inputType" />
        <attr name="android:textAppearance" />
        <attr name="android:focusable" />
        <attr name="pieErrorEnabled" format="boolean" />
        <attr name="pieHintEnabled" format="boolean" />
        <attr name="pieText" format="string" />
        <attr name="piePrefix" format="string" />
        <attr name="pieHint" format="string" />
        <attr name="pieError" format="string" />
        <attr name="pieLines" format="integer" />
        <attr name="pieMaxLines" format="integer" />
    </declare-styleable>

    <declare-styleable name="SwitchText">
        <attr name="android:inputType" />
        <attr name="android:textAppearance" />
        <attr name="android:hint" />
        <attr name="stTextOn" format="string" />
        <attr name="stTextOff" format="string" />
        <attr name="stError" format="string" />
        <attr name="stIsChecked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ButtonLoading">
        <attr name="android:text" />
        <attr name="isLoading" format="boolean" />
        <attr name="ietStyle" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
            <enum name="ternary" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ButtonImageLabel">
        <attr name="bitLabel" format="string" />
        <attr name="bitLeftIcon" format="reference" />
        <attr name="bitRightIcon" format="reference" />
        <attr name="bitRightIconVisible" format="boolean" />
    </declare-styleable>


    <declare-styleable name="MenuModuleView">
        <attr name="menumoduleTitle" format="string" />
        <attr name="menumoduleIconEnable" format="reference" />
        <attr name="menumoduleIconDisable" format="reference" />
        <attr name="menumoduleEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AutoFormatEditText">
        <attr name="isDecimal" format="boolean" />
    </declare-styleable>

</resources>