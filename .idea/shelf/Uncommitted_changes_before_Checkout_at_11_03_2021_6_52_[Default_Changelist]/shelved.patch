        txt_no_lp_sktt_edit.text = getLpWithoutSktt?.no_lp\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    companion object {\r\n        const val EDIT_SKTT = \"EDIT_SKTT\"\r\n    }\r\n}
